{
  "analysis": {
    "analyzer": {
      "sciname": {
        "type": "custom",
        "tokenizer": "keyword",
        "filter": [
          "sciname_filter",
          "lowercase"
        ]
      },
      "autocomplete_search": {
        "type": "custom",
        "tokenizer": "standard",
        "filter": [
          "lowercase"
        ]
      },
      "tokenized": {
        "type": "custom",
        "tokenizer": "standard",
        "filter": [
          "lowercase"
        ]
      },
      "autocomplete": {
        "type": "custom",
        "tokenizer": "keyword",
        "filter": [
          "lowercase",
          {
            "type": "edge_ngram",
            "min_gram": 1,
            "max_gram": 30
          }
        ]
      },
      "autocomplete_tokenized": {
        "type": "custom",
        "tokenizer": "standard",
        "filter": [
          "lowercase",
          {
            "type": "edge_ngram",
            "min_gram": 1,
            "max_gram": 20
          }
        ]
      },
      "ngram_tokenized_search": {
        "type": "custom",
        "tokenizer": "standard",
        "filter": [
          "lowercase"
        ]
      },
      "vernacular": {
        "type": "custom",
        "tokenizer": "standard",
        "filter": [
          "asciifolding",
          "lowercase"
        ]
      }
    }
  }
}