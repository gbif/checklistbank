        <gbif-cli.version>1.1</gbif-cli.version>
        <gbif-common.version>0.52</gbif-common.version>
        <gbif-common-mybatis.version>1.2-SNAPSHOT</gbif-common-mybatis.version>
        <!-- matches solr 5.4.x -->
        <gbif-common-search.version>0.36-SNAPSHOT</gbif-common-search.version>
        <gbif-common-service.version>0.18-SNAPSHOT</gbif-common-service.version>
