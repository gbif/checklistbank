    <artifactId>checklistbank-motherpom</artifactId>
    <version>2.96-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>ChecklistBank Mother POM</name>
    <description>The projects to provide the ChecklistBank web service including solr search, it's
        client, persistence
        service layer and the index builder
    </description>
    <url>http://github.org/gbif/checklistbank</url>

    <scm>
        <connection>scm:git:git@github.com:gbif/checklistbank.git</connection>
        <url>https://github.com/gbif/checklistbank</url>
        <developerConnection>scm:git:git@github.com:gbif/checklistbank.git</developerConnection>
        <tag>HEAD</tag>
    </scm>

    <distributionManagement>
        <site>
            <id>gh-pages</id>
            <url>http://gbif.github.io/checklistbank/</url>
        </site>
    </distributionManagement>

    <properties>
        <java.version>1.8</java.version>
        <!-- GBIF Motherpom -->
        <release.tagNameBase>checklistbank</release.tagNameBase>
        <main.basedir>${project.basedir}</main.basedir>

        <!-- datasource defaults, time in milliseconds -->
        <checklistbank.db.poolSize>8</checklistbank.db.poolSize>
        <!-- This property controls the maximum number of milliseconds that a client will wait for a connection from the pool.
        If this time is exceeded without a connection becoming available, a SQLException will be thrown. -->
        <checklistbank.db.connectionTimeout>5000</checklistbank.db.connectionTimeout>

